{
    "openapi": "3.0.0",
    "info":{
        "title": "API Cefit",
        "description": "Descrição da API",
        "contact": {
            "email": "exemplo@exemplo" 
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:5000/v1",
            "description": "API de teste"
        }
    ],
    "paths": {
        "/user/{id}/readOne": {
            "get": {
                "summary": "Ver um usuário pelo id",
                "description": "Essa rota será responsável para visualizar um usuário",
                "tags": ["User"],
                "parameters": [
                {
                    "name": "id",
                    "in": "path",
                    "description": "Id para busca",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
                ],
                "security": [{"bearerAuth": []}],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Usuário não cadastrado/Ocorreu um erro",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/readAll":{
            "get": {
                "summary": "Ver todos os usuários",
                "description": "Essa rota é responsável por ver todos os usuários",
                "tags": ["User"],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/User"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Ocorreu um erro ao recuperar a lista de usuários",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/createUser":{
            "post":{
                "summary": "Criar novo usuário",
                "description": "Rota responsável por criar um novo usuário",
                "tags": ["User"],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json":{
                            "schema":{
                                "$ref": "#/components/schemas/User"
                            },
                            "examples": {
                                "users": {
                                    "value": {
                                        "nome": "Luiz",
                                        "email": "luiz@teste.com",
                                        "birth": "31/05/2003",
                                        "CPF": "12345678910",
                                        "senha": "luizteste123"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Usuário cadastrado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Ocorreu um erro ao tentar criar um usuário",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "errorEmail": {
                                        "value": {
                                            "message": "Email já cadastrado",
                                            "error": "ValidationError"
                                        }
                                    },
                                    "errorCPF": {
                                        "value": {
                                            "message": "CPF já cadastrado",
                                            "error": "ValidationError"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/{id}/updateUser": {
            "patch": {
                "summary": "Atualiza usuário",
                "description": "Essa rota é responsável por atualizar um usuário pelo id",
                "tags": ["User"],
                "parameters": [
                {
                    "name": "id",
                    "in": "path",
                    "description": "Id do usuário a ser atualizado",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
                ],
                "security": [{"bearerAuth": []}],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/User"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Usuário atualizado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "$ref": "#/components/schemas/User"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Ocorreu um erro na tentativa da atualização",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/{id}/deleteUser": {
            "delete": {
                "summary": "Deleta usuário",
                "description": "Essa rota é responsável por deletar um usuário através do id",
                "tags": ["User"],
                "parameters": [
                {
                    "name": "id",
                    "in": "path",
                    "description": "Id do usuário a ser deletado",
                    "required": true,
                    "schema": {
                        "type": "string"
                    }
                }
                ],
                "security": [{"bearerAuth": []}],
                "responses": {
                    "200": {
                        "description": "Usuário deletado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "$ref": "#/components/schemas/User"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Usuário não encontrado ou ocorreu um erro na remoção",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "userNotFound": {
                                        "value": {
                                            "message": "User não encontrado",
                                            "error": "UserNotFoundError"
                                        }
                                    },
                                    "deletionError": {
                                        "value": {
                                            "message": "Erro ao tentar deletar user",
                                            "error": "ValidationError"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/login": {
            "post": {
                "summary": "Login de usuário",
                "description": "Essa rota é responsável por realizar o login do usuário",
                "tags": ["User"],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "description": "Email do usuário"
                                    },
                                    "senha": {
                                        "type": "string",
                                        "description": "Senha do usuário"
                                    }
                                },
                                "required": ["email", "senha"]
                            },
                            "examples": {
                                "loginExemplo": {
                                    "value": {
                                        "email": "luiz@teste.com",
                                        "senha": "luizteste123"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login realizado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "status": {
                                            "type": "boolean"
                                        },
                                        "token": {
                                            "type": "string"
                                        },
                                        "user": {
                                            "$ref": "#/components/schemas/User"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Login ou senha errados, ou erro durante o login",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        },
                                        "status": {
                                            "type": "boolean"
                                        }
                                    }
                                },
                                "examples": {
                                    "wrongCredentials": {
                                        "value": {
                                            "message": "Login ou senha errados",
                                            "status": false,
                                            "error": "Invaid credentials"
                                        }
                                    },
                                    "loginError": {
                                        "value": {
                                            "message": "Erro durante o login",
                                            "status": false,
                                            "error": "Unknown error"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/anamnese/createAnamnese": {
            "post": {
                "summary": "Criar anamnese",
                "description": "Rota responsável pela criação de anamnese",
                "tags": ["Anamnese"],
                "security": [{"bearerAuth": []}],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Anamnese"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Anamnese criada com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "anamnese": {
                                            "$ref": "#/components/schemas/Anamnese"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Usuário já possui anamnese ou erro ao criar anamnese",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "existingAnamnese": {
                                        "value": {
                                            "message": "Usuário já possui uma anamnese"
                                        }
                                    },
                                    "createError": {
                                        "value": {
                                            "message": "Erro ao criar anamnese",
                                            "error": "Unknown error"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/anamnese/{userId}": {
            "get": {
                "summary": "Obter anamnese por id do usuário",
                "description": "Rota responsável por buscar uma anamnese específica através do id do usuário",
                "tags": ["Anamnese"],
                "security": [{"bearerAuth": []}],
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "required": true,
                        "description": "Id do usuário para o qual a anamnese vai ser recuperada",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Anamnese recuperada com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "anamnese": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Anamnese"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Erro ao buscar anamnese",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "fetchError": {
                                        "value": {
                                            "message": "Erro ao buscar anamnese",
                                            "error": "Unknown error"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/anamnese/{id}": {
            "delete": {
                "summary": "Deletar anamnese",
                "description": "Rota responsável por deletar a anamnese pelo id",
                "tags": ["Anamnese"],
                "security": [{"bearerAuth": []}],
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "required": true,
                        "description": "Id do usuário para o qual a anamnese vai ser recuperada",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Anamnese deletada com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "$ref": "#/components/schemas/Anamnese"                                            
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Erro ao deletar anamnese ou anamnese não encontrada",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "notFound": {
                                        "value": {
                                            "message": "Anamnese não encontrada"
                                        }
                                    },
                                    "deleteError": {
                                        "value": {
                                            "message": "Erro ao deletar anamnese",
                                            "error":"Unknown error"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/anamnese/updateAnamnese": {
            "put": {
                "summary": "Atualizar anamnese",
                "description": "Rota responsável por atualizar a anamnese existente através do id",
                "tags": ["Anamnese"],
                "security": [{"bearerAuth": []}],
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "required": true,
                        "description": "Id do usuário para o qual a anamnese vai ser recuperada",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Anamnese"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Anamnese atualizada com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "anamnese": {
                                            "$ref": "#/components/schemas/Anamnese"                                            
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Erro ao atualizar anamnese",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "examples": {
                                    "updateError": {
                                        "value": {
                                            "message": "Erro ao atualizar anamnese",
                                            "error": "Unknown error"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components":{
        "schemas":{
            "User": {
                "type": "object",
                "properties": {
                    "nome": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "birth": {
                        "type": "string",
                        "format": "date"
                    },
                    "CPF": {
                        "type": "string"
                    },
                    "senha": {
                        "type": "string"
                    }
                }
            },
            "Anamnese": {
                "type": "object",
                "properties": {
                    "userId": {
                        "type": "string"
                    },
                    "weight": {
                        "type": "string"
                    },
                    "motivation": {
                        "type": "string"
                    },
                    "activityFreq": {
                        "type": "string"
                    },
                    "date": {
                        "type": "string",
                        "format": "date"
                    },
                    "diet": {
                        "type": "string"
                    },
                    "observacoes": {
                        "type": "string"
                    }
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
}